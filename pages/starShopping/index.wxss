.containor{
  width: 100%;
  height: 100vh;
  background-color: #f6f6f6;
}
.starButtom{
  position: absolute;
  bottom: 0;
  width: 100vw;
  height: 8vh;
}
.starButtom .starButtomContent{
  width: 75vw;
  height: 8vh;
  line-height: 8vh;
  color: #a77d6e;
  font-size: 35rpx;
  background-color: #fff;
  border: 1px solid #e5e5e5;
}
.starButtom .starButtomContent text{
  margin-left: 50rpx;
}
.starContent .myGoods{
  position: relative;
  width: 100vw;
  height: 15vh;
  background-color: #fff;
  border: 1px solid #e5e5e5;
}
.starContent .myGoods .delete{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  margin-top: -25rpx;
}
.starContent .myGoods .showPicture{
  width: 35vw;
  height: 15vh;
  position: absolute;
  left: 150rpx;
}
.starContent .myGoods .goodsName{
  position: absolute;
  left: 58vw;
  top: 2vh;
  font-size: 30rpx;
}
.starContent .myGoods .price{
  position: absolute;
  bottom: 1vh;
  right: 12vw;
  color: #f40;
}
.starButtom .starButtomButton{
  width: 25vw;
  height: 8vh;
  line-height: 8vh;
  color: #fff;
  background-color: #a77d6e;
  text-align: center;
  position: absolute;
  bottom: 0;
  right: 0;
}

.containor .starContent .active{
  border: 1px solid #f40;
  /* background-color: #f40;  */
}

.containor .starContent .myGoods icon{
  position: absolute;
  top: 35rpx;
  right: 20rpx;
  display: none;
}
.containor .starContent .active icon{
  display: block;
}